pipeline
{
    agent any
    stages
    {
    stage ('cloning code from github')
    {
    steps
    {
       sh 'touch xyz.txt'
    }
    }
    stage ('building the code')
    {
    steps
    {
       sh 'touch manupulate.txt'
    }
    }
    stage ('testing the code')
    {
    steps
    {
       sh 'touch bca.txt'
    }
    }
    stage ('deploying the code')
    {
    steps
    {
       sh 'touch cba.txt' 
    }
    }


    }
}

